.main
  .pageTop
    .title
      %h1 OTHELLO
    .nameBox
      %input{type: "text",value: "なまえ",class: "inputNameBlack",placeholder: "なまえ",maxlength: "5",pattern: "(?=.*?[a-z])(?=.*?[A-Z])(?=.*?\d)[a-zA-Z\d]"}
      %input{type: "text",value: "なまえ",class: "inputNameWhite",placeholder: "なまえ",maxlength: "5",pattern: "(?=.*?[a-z])(?=.*?[A-Z])(?=.*?\d)[a-zA-Z\d]"}
    .changeBtn
      %button{value: "CHANGE",class: "changeButton",type:"button"} 石を変更する
    .gameStartBtn
      %button{value: "START",class: "startButton",type:"button"} ゲームスタート
  .player-wrapper
    .player-black.turnPlayer#playerData
      .player-black-name#playerName なまえ
      .player-black-counter#blackCounter 2
    .pass
      .pass-btn.turnBlack#passBtn パス
    .player-white#playerData
      .player-white-name#playerName なまえ
      .player-white-counter#whiteCounter 2
  .wrap
    .gameBoard
      .topPageBtn
        %button{class: "resetBtn",name: "REPLAY",type:"button"}
      .time
        .timeResult
        .timeWrap
          %a#timeMin<>00
          %a :
          %a#timeTenSec<>0
          %a#timeSec<>0
          %a .
          %a#timeTithe<>0
